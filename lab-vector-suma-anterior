#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

struct vector{
	int vec[50];
};

void mostrar(vector v[],int n, int m)
{
	int i,j;
	for(i=0;i<n;i++){
		for(j=0;j<m;j++){
			cout<<"\t"<<v[i].vec[j];
		}
	}
}


void sumaA(vector v[], int n,int m)
{
	int i,j,k=-1,s=1,h=0;
	for(i=0;i<n;i++)
	{
		for(j=0;j<m;j++)
		{
			h=k+s;
			k=s;
			s=h;
			v[i].vec[j]=h;
		}
	}
}
void main()
{
	int n,m; vector v[50];
	cout<<"ingrese el tamano del vector ";
	cin>>n;
	cout<<"ingrese el tamaÃ±o del vector interno ";
	cin>>m;
	sumaA(v,n,m);
	mostrar(v,n,m);
	getch();
}
